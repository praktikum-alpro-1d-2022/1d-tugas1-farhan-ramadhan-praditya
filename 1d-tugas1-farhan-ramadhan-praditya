#include <iostream>
using namespace std;

int main()
{
	cout << "Tanpa memakai variabel tambahan" << endl;
	cout << "\n" << endl;
	int a = 3;
	int b = 5;
	cout << "Nilai awal A	:" << a << endl;
	cout << "Nilai awal B	:" << b << endl;
	a = a + b;
	b = a - b;
	a = a - b;
	cout << "Nilai akhir A	:" << a << endl;
	cout << "Nilai akhir B	:" << b << endl;
  
  return 0;
	cout << "Memakai variabel tambahan" << endl;
	cout << "\n" << endl;
	int a = 3;
	int b = 5;
	int temp;
	cout << "Nilai awal A	: " << a << endl;
	cout << "Nilai awal B	: " << b << endl;
	temp = a;
	a = b;
	b = temp;
	cout << "Nilai akhir A	:" << a << endl;
	cout << "Nilai akhir B	:" << b << endl;

	return 0;
}
